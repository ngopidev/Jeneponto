<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ngopi.jeneponto"    >

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:banner="@drawable/jeneponto"
        android:icon="@drawable/jeneponto"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <!--this for splash screen-->
        <activity android:name=".SplashScreen"
            android:label="Jeneponto"
            android:theme="@style/AppTheme.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity
        android:name=".Home"
        android:label="Jeneponto"
        android:theme="@style/AppTheme"/>
        <activity android:name=".Contact"
            android:label="Jeneponto"
            android:theme="@style/AppTheme"/>
        <activity android:name=".Bahasa"
            android:label="Jeneponto"
            android:theme="@style/AppTheme"/>
        <activity android:name=".MianActivity"
            android:label="@string/app_name"/>
        <activity android:name=".Kalkulator"
            android:label="@string/app_name"/>
    </application>

</manifest>